public class SupervTec extends Empleado{
    
    public SupTec(String nombresYapellidos, double cedula) {
        super(nombresYapellidos, cedula);
    }

    public double salarioSupervisor(int anios){
        double adicional;
        if(anios  == 0){
            return salario;
        }
        else{
            adicional = salario * 0.05;
            salario +=adicional;
            return salario;
        }
    }
   

    public SupervTec(double salario) {
        super(salario);
    }
}
